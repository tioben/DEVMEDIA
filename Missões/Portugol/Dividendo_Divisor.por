programa {
    funcao inicio()
    {
    //Declaração de variáveis
    real dividendo
    real divisor
    real resultado

    //Lendo dados
    escreva("Digite um dividendo: ")
    leia(dividendo)
    escreva("Digite um divisor: ")
    leia(divisor)
    
    //Cálculo
    resultado = dividendo/divisor

    //Escrevendo dados na tela
    escreva(resultado)
}